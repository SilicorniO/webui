    "@typescript-eslint/parser": "2.27.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "6.8.0",
