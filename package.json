{
  "name": "js-webui",
  "version": "1.0.5",
  "main": "build/webui-dist.js",
  "description": "Easier way to set the position of the views/tags in a website.  Built with basic Javascript, it should be working in most of browsers.",
  "private": false,
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "dist": "webpack --config webpack.prod.js",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "npx jest --runInBand",
    "test:coverage": "npx jest --coverage --runInBand",
    "lint:fix": "eslint --fix src --ext 'src/**/*.{js,ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SilicorniO/webui.git"
  },
  "author": "Javier@Silicornio",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/SilicorniO/webui/issues"
  },
  "homepage": "https://github.com/SilicorniO/webui#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/puppeteer": "5.4.3",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "babel-core": "6.26.3",
    "babel-loader": "8.2.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "jest-puppeteer": "4.4.0",
    "prettier": "2.2.1",
    "puppeteer": "8.0.0",
    "ts-jest": "26.5.3",
    "ts-loader": "8.0.17",
    "typescript": "4.2.3",
    "webpack": "5.24.3",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.1",
    "webpack-merge": "5.7.3"
  }
}
